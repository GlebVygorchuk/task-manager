{
    "builds": [
        {
            "src": "task-manager/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "task-manager/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/task-manager/$1"
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}